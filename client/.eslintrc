{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "react-app",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "printWidth": 85,
        "trailingComma": "es5",
        "singleQuote": true,
        "arrowParens": "always",
        "semi": false
      }
    ],
    "no-console": "off",
    "no-use-before-define": "off",
    "semi": [
      "warn",
      "never"
    ],
    "react/prop-types": 1
  }
}